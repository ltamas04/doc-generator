<table class="table table-striped szures-tabla" id="szures-tabla">
  <thead>
    <tr>
      <th>Mintavétel dátuma</th>
      <th>Mintavétel helye</th>
      <th>Gép</th>
      <th>Szondafűtés</th>
      <th>Gázelőkészítő</th>
      <th>Fűtött vezeték</th>
      <th>Tömörítettség ellenőrzés</th>
      <th>Pontosság</th>
      <th>Fujitsu Siemens hordozható számítógép</th>
      <th>Aláírás</th>
    </tr>
  </thead>
  <tbody>
    {{#each docs}}
    <tr>
      <td>{{mintavetel_datuma}}</td>
      <td>{{mintavetel_helye}}</td>
      <td>{{horiba}}</td>
      <td>{{szondafutes}}</td>
      <td>{{gazelokeszito}}</td>
      <td>{{futottvezetek}}</td>
      <td>{{tomoritett}}</td>
      <td>{{pontossag}}</td>
      <td>{{fujitsu}}</td>
      <td>{{alairas}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
<button id="tableExport">Export</button>
